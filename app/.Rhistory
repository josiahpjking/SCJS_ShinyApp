table(demograph$qratpol, demograph2$qratpol,exclude=NULL)
table(demograph$qworr_01, demograph2$qworr_01,exclude=NULL)
table(demograph$poloprel, demograph2$poloprel,exclude=NULL)
table(demograph$qworr_01, demograph2$qworr_01,exclude=NULL)
table(demograph$compol2, demograph2$compol2,exclude=NULL)
table(demograph$polpres, demograph2$polpres,exclude=NULL)
tidy_df<-demograph
rm(demograph2)
#######
##check variables are present across years.
#######
tidy_df %>% group_by(year) %>% summarise_all(
funs(sum(!is.na(.)))) %>%
gather(., key="variable",value="number_obs",-year) -> year_counts
#this will show the number of non NA observations across years for variables matching the string
year_counts %>% filter(grepl("qsfdark",variable))
#this summarises whether a variable has at least 1 non NA observation for each year.
year_counts %>% group_by(variable) %>%
summarise(
no_na = !(any(number_obs==0))
) -> variable_across_years
######
#create pdiv proportion data
######
#these are all the variables we are interested in looking at proportions of, with individual weightings to be applied
tidy_df %>% select(prevviolent, prevsurveycrime, polpres, qsfdark, matches("qpolconf|qs2area|qsfnigh|qratpol|polop|compol|qworr|qaco_|lcpeop|qhworr|dconf")) %>% names() -> vars
#the prevproperty crime variable requires applying the household weighting
hhdvars = list("prevproperty","wgtghhd")
test5_vars<-list(vars=vars, wgtg=rep("wgtgindiv",length(vars)))
test5_vars<-mapply(append, test5_vars, hhdvars, SIMPLIFY = FALSE)
#the functions here will create tables for us
source("scripts/functions_yeartables.R")
#we need to reference the design effects for calculating CIs
design_effects <- data.frame(year=as.character(c(1,2,3,4,5,6)),des_effect=c(1.5,1.5,1.5,1.3,1.2,1.34))
#this creates tables for each police division. it's a little time consuming (15 seconds)
create_split_tables(tidy_df, "police_div",test5_vars, design_effects) -> pdiv_tables
#we also want the national averages for all these, so we want to create tables not split by police divisions.
#then set the breaks variable (as national average), remove all proportions of values!=1 of variables (the create_split_tables function does this for you, but this one does not)
#then join it to the other table
create_tables(df = tidy_df, vars_and_wgtgs = test5_vars,dsgn_df = design_effects) %>%
mutate(breaks="National Average") %>%
filter(value==1) %>%
bind_rows(pdiv_tables,.) -> pdiv_tables
#don't worry about warnings, this is from joining data.
#set year
pdiv_tables$year <- factor(pdiv_tables$year)
levels(pdiv_tables$year)
levels(pdiv_tables$year)<-c("2008/09","2009/10","2010/11","2012/13","2014/15","2016/17")
variable_info<-read.csv("variables_full.csv")
variable_info$variable<-tolower(variable_info$variable)
pdiv_tables<-left_join(pdiv_tables,variable_info)
head(pdiv_tables)
pdiv_tables$wrappedv <- sapply(pdiv_tables$label, FUN = function(x) {paste(strwrap(x, width = 25), collapse = "<br>")})
pdiv_tables$wrapped_name <- sapply(pdiv_tables$name_trunc, FUN = function(x) {paste(strwrap(x, width = 25), collapse = "<br>")})
#set variable
pdiv_tables$variable<-factor(pdiv_tables$variable)
levels(pdiv_tables$variable)
pdiv_tables$variable<-factor(pdiv_tables$label)
levels(pdiv_tables$variable)
saveRDS(pdiv_tables, "pdiv_dash/v9/data/pdiv8.1_current.rds")
####
##Making dummy data
####
pdiv_tables %>% select(-count) %>% mutate(
p=sample(jitter(p)),
percentage=p*100,
ci = (sqrt((p*(1-p))/samplesize))*1.96*des_effect,
low = percentage-(ci*100),
high = percentage+(ci*100)
) -> pdiv_scrambled
saveRDS(pdiv_scrambled, "pdiv_dash/v9/data/pdiv8.1_scrambled.rds")
